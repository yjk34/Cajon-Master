float distOf(CCPoint p1, CCPoint p2) {
	return sqrt ((float) ((p1.x - p2.x) * (p1.x - p2.x) + (p1.y - p2.y) * (p1.y - p2.y)) );
}

int mapMoveToSpeed(float avgMove) {
	return (int)((avgMove - 1 / 0.8) * 60 + 80);
