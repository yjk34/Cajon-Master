class MatchingTemplate {
	public String templateName;
	public Vector<CCPoint> samples;

	public MatchingTemplate(String name, Vector<CCPoint> points) {
		templateName = name;
		samples = points;
	}
}

class MatchingResult {
	public String templateName;
	public double templateScore;

	public MatchingResult(String name, double score) {
		templateName = name;
		templateScore = score;
	}
}

class CCPoint {
	public double x;
	public double y;
	public CCPoint(double _x, double _y) {
		x = _x;
		y = _y;
	}
}

void doGestureRecognizeAndClearList() {
  MatchingResult mr = gr.checkMatchingGesture(pntsList);
  
  if (mr.templateScore < TEMPLTATE_SCORE_THRESH) {
    println("The gesture is ignored.");
    println("Ignore gesture is: " + mr.templateName);
    println("Ignore gesture score is: " + mr.templateScore);
  } else {
    println("This gesture is: " + mr.templateName);
    println("This gesture score is: " + mr.templateScore);
    if (mr.templateName.equals("Line")) {
      analysisLineGesture();
    } else if (mr.templateName.equals("Circle")) {
      analysisCircleGesture();
    } else if (mr.templateName.equals("Zeta")) {
    	;
    } else if (mr.templateName.equals("Vertical")) {
    	isPause = isPause ? false : true;
    }
  }
  
  for(int i=0; i<pntsList.size(); i++)
  	print("new CCPoint(" + pntsList.get(i).x + ", " + pntsList.get(i).y + "), ");
  print("\n");

  pntsList.clear();
}

void analysisLineGesture() {
	float avgMove = 0;
	for(int i=1; i<pntsList.size(); i++) 
		avgMove += distOf(pntsList.get(i), pntsList.get(i-1)) / (pntsList.size()-1);
	println("AVG MOVE: " + avgMove);
	println("MAP SPEED: " + mapMoveToSpeed(avgMove));
	curSpeed = mapMoveToSpeed(avgMove);
}

